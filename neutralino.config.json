{
  "applicationId": "js.neutralino.sample",
  "defaultMode": "window",
  "port": 0,
  "url": "/dist/",
  "enableServer": true,
  "enableNativeAPI": true,
  "logging": {
    "enabled": true,
    "writeToLogFile": true
  },
  "nativeBlockList": [],
  "globalVariables": {
    "TEST": "Test Value"
  },
  "modes": {
    "window": {
      "title": "test-app",
      "width": 1300,
      "height": 860,
      "minWidth": 400,
      "minHeight": 200,
      "fullScreen": false,
      "alwaysOnTop": false,
      "icon": "/neutralino.png",
      "enableInspector": true,
      "borderless": false,
      "maximize": false,
      "hidden": false,
      "resizable": true,
      "exitProcessOnClose": true
    }
  },
  "cli": {
    "binaryName": "test-app",
    "resourcesPath": "/dist/",
    "clientLibrary": "/dist/js/neutralino.js",
    "binaryVersion": "3.0.0",
    "clientVersion": "2.0.0"
  }
}
